{
  "base_urls": "https://qa-bpm.se2.com",
    "regions": {
      "dev": "https://login.qa.zinnia.com/",
      "QA": "https://login.qa.zinnia.com/",
      "prod": "https://login.qa.zinnia.com/"
    },
    "apis": [
      {
        "name": "Get OAuth Token",
        "endpoint": "/oauth/token",
        "method": "POST",
        "api_type": "Authentication",
        "test_type": ["smoke", "regression"],
        "environments": ["dev", "QA", "prod"],
        "payload": {
          "audience": "https://qa.api.zinnia.io",
          "grant_type": "client_credentials",
          "client_id": "UuMribrP3e8zVhfHy6wPJ25lVYh1CWN3",
          "client_secret": "WQ4ZBFI0RaYnhlhrZBdw4ltrMk8IKT9bDm1DX_U28doSk4F4Au5KiAsoBCSNYWix"
        },
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded"
        }
      },
      {
        "name": "Get Case Details",
        "endpoint": "/case/v1/cases/{caseID}",
        "method": "GET",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["dev"],
        "headers": {
          "Content-Type": "application/json"
        },
        "default_case_id": "CA0000370039"
      }, 
      {
        "name": "Search Cases by Policy Number",
        "endpoint": "/case/v1/cases/search",
        "method": "POST",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["QA", "prod"],
        "base_url": "https://qa-bpm.se2.com",
        "payload": {
          "policyNumber": "571025434"
        },
        "headers": {
          "Content-Type": "application/json"
        }
      },
      {
        "name": "Search Cases by Agent Name",
        "endpoint": "/case/v1/cases/search",
        "method": "POST",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["QA", "prod"],
        "base_url": "https://qa-bpm.se2.com",
        "payload": {
          "agentFirstName": "ROBERT",
          "limit": 1000,
          "offset": 0,
          "sortDirection": "desc",
          "sortBy": "createdAt"
        },
        "headers": {
          "Content-Type": "application/json"
        }
      },
      {
        "name": "Get Assigned Tasks",
        "endpoint": "/case/v1/tasks/assigned",
        "method": "GET",
        "api_type": "Case Management",
        "test_type": ["smoke", "regression"],
        "environments": ["dev", "QA"],
        "base_url": "https://qa.api.zinnia.io",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      {
        "name": "Post Task Message",
        "endpoint": "/case/v1/tasks/message",
        "method": "POST",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["QA"],
        "base_url": "https://qa.api.zinnia.io",
        "payload": {
          "taskType": "SUITABILITY_REVI",
          "carrier": "MASS",
          "category": "SCHEDULED_REASON",
          "messageId": "DOCUMENT_MISSING",
          "message": "Document is missing for SUITABILITY_REVIEW for MASS"
        },
        "headers": {
          "Content-Type": "application/json"
        }
      },
      {
        "name": "Get Assignee Summary",
        "endpoint": "/case/v1/cases/{caseID}/tasks/assignee-summary",
        "method": "GET",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["QA"],
        "base_url": "https://qa.api.zinnia.io",
        "headers": {
          "Content-Type": "application/json"
        },
        "default_case_id": "CA0000425395"
      },
      {
        "name": "Get Case Exceptions",
        "endpoint": "/case/v1/exceptions/cases/{caseID}",
        "method": "GET",
        "api_type": "Case Management",
        "test_type": ["smoke"],
        "environments": ["QA"],
        "base_url": "https://qa.api.zinnia.io",
        "headers": {
          "Content-Type": "application/json"
        },
        "default_case_id": "CA0000006344"
      }
    ]
  }
  